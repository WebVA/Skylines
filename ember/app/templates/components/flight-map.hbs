{{#if noCanvas}}
  <b>
    Your browser is too old to display this map.<br/>
    Please update to a current version.
  </b>
{{else}}
  {{layer-switcher map=map}}

  {{#if fullscreenElement}}
    {{fullscreen-button fullscreenElement=fullscreenElement}}
  {{/if}}

  {{#if flights}}
    {{playback-button}}
    {{flight-path-layer map=map flights=flights}}
    {{phase-highlight-composer map=map}}
    {{plane-icons-composer map=map}}
  {{/if}}

  {{#if contests}}
    {{contest-layer map=map contests=contests}}
  {{/if}}
{{/if}}